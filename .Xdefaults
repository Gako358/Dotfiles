URxvt.geometry: 120x55
URxvt.intensityStyles:	false
URxvt.background:       [99]#141c21
URxvt.depth:		32
URxvt.scrollBar:	false
URxvt.cursorColor:	white

!!Hack Bold
U!!Rxvt.font: xft:Hack Nerd Font:pixelsize=11:antialias=true:hinting=true
!!URxvt.boldFont: xft:Hack Nerd Font:bold:pixelsize=11:antialias=true:hinting=true
!!URxvt.italicFont: xft:Hack Nerd Font:italic:pixelsize=11:antialias=true:hinting=true
!!URxvt.boldItalicFont: xft:Hack Nerd Font:bold:italic:pixelsize=11:antialias=true:hinting=true
!!URxvt.letterSpace: 1

!!DroidSansMono
URxvt.font: xft:DroidSansMono Nerd Font:pixelsize=11:antialias=true:hinting=true
URxvt.boldFont: xft:DroidSansMono Nerd Font:bold:pixelsize=11:antialias=true:hinting=true
URxvt.italicFont: xft:DroidSansMono Nerd Font:italic:pixelsize=11:antialias=true:hinting=true
URxvt.boldItalicFont: xft:DroidSansMono Nerd Font:bold:italic:pixelsize=11:antialias=true:hinting=true
URxvt.letterSpace: 1
